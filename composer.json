{
  "name": "contentful/contentful-management",
  "description": "SDK for the Contentful Content Management API",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.0",
    "contentful/contentful": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "php-vcr/phpunit-testlistener-vcr": "^3.0",
    "php-vcr/php-vcr": "1.3.1",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "jakub-onderka/php-console-highlighter": "^0.3.2",
    "nikic/php-parser": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Contentful\\": "src/",
      "Contentful\\Tests\\Management\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.9.0-dev"
    }
  },
  "scripts": {
      "test": "vendor/bin/phpunit"
  },
  "suggest": {
    "nikic/php-parser": "Allows generating content type classes"
  }
}
